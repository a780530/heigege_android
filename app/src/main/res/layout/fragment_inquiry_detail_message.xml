<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f3f3f3"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="vertical">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#f4f4f4"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="78px"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="9.5px">

                <TextView
                    android:id="@+id/tv_kun_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="18px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="32px" />

                <Button
                    android:id="@+id/bt_kun_key"
                    android:layout_width="104px"
                    android:layout_height="40px"
                    android:layout_marginLeft="16px"
                    android:layout_toRightOf="@id/tv_kun_code"
                    android:background="@drawable/button_style"
                    android:text="1329A"
                    android:textColor="@color/red_translucent"
                    android:textSize="24px" />

                <TextView
                    android:id="@+id/tv_kun_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="20px"
                    android:layout_marginTop="10px"
                    android:drawableLeft="@mipmap/ic_common_cutton"
                    android:drawablePadding="7.24px"
                    android:text="锯齿细绒棉"
                    android:textSize="24px" />
            </com.zhy.autolayout.AutoRelativeLayout>

        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="产地"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_origin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="主体颜色级"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="总公重"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_KG"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="马克隆档次"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_horse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="长度"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_length"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="长度整齐率"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_lengthQ"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="回潮率"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_moisture"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="含杂率"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_trash"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="强力"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_breakLoadAverage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="Rd"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_rdAverage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:orientation="horizontal">

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="+b"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_plusBAverage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="360px"
                android:layout_height="80px"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="棉花年度"
                    android:textColor="@color/detailsColor"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_kun_createYear"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#000"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>
        </com.zhy.autolayout.AutoLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:background="#d1d1d1" />

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ll_background_normal"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingBottom="20px"
            android:paddingTop="20px">

            <TextView
                android:layout_width="142px"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="24px"
                android:text="公检仓库"
                android:textColor="@color/detailsColor"
                android:textSize="28px" />

            <TextView
                android:id="@+id/tv_kun_checkStorage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="24px"
                android:layout_marginRight="20px"
                android:text="--"
                android:textColor="#000"
                android:textSize="28px" />
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>

</com.zhy.autolayout.AutoLinearLayout>
