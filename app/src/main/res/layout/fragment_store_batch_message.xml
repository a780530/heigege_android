<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="100px"
    android:background="#f4f4f4"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/sl_haveData"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <com.zhy.autolayout.AutoFrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">

                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="94px"
                        android:background="@color/white"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/tv_batch_batch"
                            android:layout_width="wrap_content"
                            android:layout_height="44px"
                            android:layout_marginLeft="18px"
                            android:text="65092161019(180/1)"
                            android:textColor="#000"
                            android:textSize="32px" />

                        <Button
                            android:id="@+id/bt_batch_key"
                            android:layout_width="104px"
                            android:layout_height="40px"
                            android:layout_marginLeft="16px"
                            android:background="@drawable/button_style"
                            android:text="1329A"
                            android:textColor="@color/red_translucent"
                            android:textSize="24px" />

                    </com.zhy.autolayout.AutoLinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1px"
                        android:background="#f4f4f4" />

                    <com.zhy.autolayout.AutoLinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/ll_background_normal"
                        android:orientation="horizontal">

                        <com.zhy.autolayout.AutoLinearLayout
                            android:layout_width="360px"
                            android:layout_height="80px"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="142px"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="24px"
                                android:text="加工"
                                android:textColor="@color/detailsColor"
                                android:textSize="28px" />

                            <TextView
                                android:id="@+id/tv_batch_origin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="24px"
                                android:text="--"
                                android:textColor="#333333"
                                android:textSize="28px" />
                        </com.zhy.autolayout.AutoLinearLayout>

                        <com.zhy.autolayout.AutoLinearLayout
                            android:layout_width="360px"
                            android:layout_height="80px"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="142px"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:text="提货/结算"
                                android:textColor="@color/detailsColor"
                                android:textSize="28px" />

                            <TextView
                                android:id="@+id/receviType"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="24px"
                                android:text="--"
                                android:textColor="#333333"
                                android:textSize="28px" />
                        </com.zhy.autolayout.AutoLinearLayout>
                    </com.zhy.autolayout.AutoLinearLayout>

                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right">

                    <ImageView
                        android:id="@+id/ivIsCheap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="15px"
                        android:layout_marginTop="22px"
                        android:src="@mipmap/ic_home_resour" />

                    <com.zhy.autolayout.AutoRelativeLayout
                        android:layout_width="200px"
                        android:layout_height="84px"
                        android:layout_marginTop="22px"
                        android:background="@mipmap/ic_common_price"
                        android:gravity="center">

                        <TextView
                            android:id="@+id/tv_store_kun_money"
                            android:layout_width="145px"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="5px"
                            android:layout_marginRight="4px"
                            android:gravity="right"
                            android:text="1500"
                            android:textColor="@color/white"
                            android:textSize="36px" />

                        <TextView
                            android:id="@+id/tv_kun_commany"
                            android:layout_width="40px"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="6px"
                            android:layout_toRightOf="@+id/tv_store_kun_money"
                            android:text="元/吨"
                            android:textColor="@color/white"
                            android:textSize="14px" />
                    </com.zhy.autolayout.AutoRelativeLayout>
                </com.zhy.autolayout.AutoLinearLayout>
            </com.zhy.autolayout.AutoFrameLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="80px"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <com.zhy.autolayout.AutoLinearLayout
                    android:layout_width="360px"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="142px"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="24px"
                        android:text="异性纤维"
                        android:textColor="#999999"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/tv_yxxy"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="24px"
                        android:layout_marginRight="20px"
                        android:text="--"
                        android:textColor="#333333"
                        android:textSize="28px" />
                </com.zhy.autolayout.AutoLinearLayout>

                <com.zhy.autolayout.AutoLinearLayout
                    android:id="@+id/go_toSts"
                    android:layout_width="360px"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="142px"
                        android:layout_height="wrap_content"
                        android:text="升贴水"
                        android:textColor="#999999"
                        android:textSize="28px" />

                    <TextView
                        android:id="@+id/sts_Num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="24px"
                        android:layout_marginRight="20px"
                        android:drawablePadding="62px"
                        android:drawableRight="@mipmap/ic_mine_back"
                        android:text="+400"
                        android:textColor="#333333"
                        android:textSize="28px" />
                </com.zhy.autolayout.AutoLinearLayout>
            </com.zhy.autolayout.AutoLinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#f4f4f4" />


            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#d1d1d1" />

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="加工厂编号"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_factoryCode"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="加工厂"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_factory"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="加工厂地址"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_factoryAddress"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="公检仓库"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_checkStorage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="所在仓库"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_storage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>


            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="联系人"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_contact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:layout_marginRight="20px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="联系电话"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_mobile"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="#d1d1d1" />

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="公检日期"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_checkDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <com.zhy.autolayout.AutoLinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/ll_background_normal"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:paddingBottom="24px"
                android:paddingTop="24px">

                <TextView
                    android:layout_width="142px"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="发布日期"
                    android:textColor="#999999"
                    android:textSize="28px" />

                <TextView
                    android:id="@+id/tv_batch_releaseDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="24px"
                    android:text="--"
                    android:textColor="#333333"
                    android:textSize="28px" />
            </com.zhy.autolayout.AutoLinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="20px"
                android:background="#f4f4f4"></View>

            <Button
                android:id="@+id/bt_count_bag"
                style="?android:attr/borderlessButtonStyle"
                android:layout_width="match_parent"
                android:layout_height="88px"
                android:background="@color/white"
                android:text="查看棉包详情"
                android:textColor="#343434"
                android:textSize="28px" />

            <View
                android:layout_width="match_parent"
                android:layout_height="20px"
                android:background="#f4f4f4"></View>
        </com.zhy.autolayout.AutoLinearLayout>
    </ScrollView>

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/nodata"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@mipmap/ic_nodata_commodity" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40px"
            android:gravity="center"
            android:text="这个商品已下架"
            android:textColor="#999999"
            android:textSize="32px" />
    </com.zhy.autolayout.AutoLinearLayout>
</com.zhy.autolayout.AutoLinearLayout>
