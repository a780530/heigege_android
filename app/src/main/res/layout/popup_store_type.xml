<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_myself"
        android:layout_width="match_parent"
        android:layout_height="94.5px"
        android:background="@drawable/ll_background_normal"
        android:clickable="true"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_myself"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="38px"
            android:layout_weight="1"
            android:text="2017新棉"
            android:textColor="#333333"
            android:textSize="26px" />

        <ImageView
            android:id="@+id/iv_myself"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="40px"
            android:src="@mipmap/ic_common_true" />
    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_recevi"
        android:layout_width="match_parent"
        android:layout_height="94.5px"
        android:background="@drawable/ll_background_normal"
        android:clickable="true"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_recevi"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="38px"
            android:layout_weight="1"
            android:text="国储棉"
            android:textColor="#333333"
            android:textSize="26px" />

        <ImageView
            android:id="@+id/iv_recevi"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="40px"
            android:src="@mipmap/ic_common_true" />
    </com.zhy.autolayout.AutoLinearLayout>
    <com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/ll_3128B"
        android:layout_width="match_parent"
        android:layout_height="94.5px"
        android:background="@drawable/ll_background_normal"
        android:clickable="true"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_3128B"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="38px"
            android:layout_weight="1"
            android:text="3128B"
            android:textColor="#333333"
